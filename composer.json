{
    "version": "1.0.1",
    "name": "spoob/unotis-laravel",
    "description": "Unotis API Laravel client library",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Spoob\\UnotisLaravel\\": "/src"
        }
    },
    "authors": [
        {
            "name": "SPOOBRU",
            "email": "info@spoob.ru"
        }
